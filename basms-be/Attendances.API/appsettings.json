{
  "AllowedOrigins": "${ALLOWED_ORIGINS}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "${DB_CONNECTION_STRING_ATTENDANCES}"
  },
  "Jwt": {
    "Key": "",
    "Issuer": "",
    "Audience": ""
  },
  "EmailSettings": {
    "Sender": "${EMAIL_SENDER}",
    "Password": "${EMAIL_PASSWORD}",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Port": 5672,
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}",
    "VirtualHost": "/"
  },
  "AWS": {
    "BucketName": "${AWS_BUCKET_FACEID_NAME}",
    "Region": "${AWS_REGION}",
    "AccessKey": "${AWS_ACCESS_KEY}",
    "SecretKey": "${AWS_SECRET_KEY}",
    "FolderPrefix": "attendances"
  },
  "FaceRecognitionApi": {
    "BaseUrl": "${FACEID_API_BASE_URL}",
    "ApiKey": "${FACEID_API_KEY}",
    "Endpoints": {
      "Register": "/api/v1/faces/register",
      "Verify": "/api/v1/faces/verify",
      "Health": "/api/v1/health"
    },
    "Timeout": 30
  },
  "HereApiSettings": {
    "ApiKey": "${HERE_API_KEY}",
    "GeocodingEndpoint": "https://geocode.search.hereapi.com/v1/geocode"
  },
  "AllowedHosts": "*"
}
