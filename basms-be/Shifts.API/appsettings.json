{
  "AllowedOrigins": "${ALLOWED_ORIGINS}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "${DB_CONNECTION_STRING_SHIFTS}"
  },
  "Jwt": {
    "Key": "",
    "Issuer": "",
    "Audience": ""
  },
  "EmailSettings": {
    "Sender": "${EMAIL_SENDER}",
    "Password": "${EMAIL_PASSWORD}",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 465
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Port": 5672,
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}",
    "VirtualHost": "/"
  },
  "AllowedHosts": "*"
}
