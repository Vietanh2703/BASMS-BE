{
  "AllowedOrigins": "${ALLOWED_ORIGINS}",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "${DB_CONNECTION_STRING_SHIFTS}"
  },
  "Jwt": {
    "Key": "",
    "Issuer": "",
    "Audience": ""
  },
  "EmailSettings": {
    "Sender": "${EMAIL_SENDER}",
    "Password": "${EMAIL_PASSWORD}",
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": 587
  },
  "AWS": {
    "BucketName": "${AWS_BUCKET_FILE_NAME}",
    "Region": "${AWS_REGION}",
    "AccessKey": "${AWS_ACCESS_KEY}",
    "SecretKey": "${AWS_SECRET_KEY}",
    "FolderPrefix": "shifts"
  },
  "RabbitMQ": {
    "Host": "${RABBITMQ_HOST}",
    "Port": 5672,
    "Username": "${RABBITMQ_USERNAME}",
    "Password": "${RABBITMQ_PASSWORD}",
    "VirtualHost": "/"
  },
  "AllowedHosts": "*"
}
